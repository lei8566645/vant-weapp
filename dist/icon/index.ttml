<view
  class="custom-class {{ classPrefix }} {{ isSrc ? 'van-icon--image':classPrefix + '-' + name}}"
  style="{{ color ? 'color: ' + color + ';' : '' }}{{ size ? 'font-size: ' + size + ';' : '' }}{{ customStyle }}"
  bind:tap="onClick"
>
  <van-info
    tt:if="{{ info !== null }}"
    info="{{ info }}"
    custom-class="van-icon__info"
  />
  <image
    tt:if="{{ isSrc }}"
    src="{{ name }}"
    mode="aspectFit"
    class="van-icon__image"
  />
</view>
