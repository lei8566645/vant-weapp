<view
  class="custom-class van-switch {{value === activeValue?'van-switch--on':''}} {{disabled?'van-switch--disabled':''}}"
  style="font-size: {{ size }}; {{ (checked ? activeColor : inactiveColor) ? 'background-color: ' + (checked ? activeColor : inactiveColor ) : '' }}"
  bind:tap="onClick"
>
  <view class="van-switch__node node-class">
    <van-loading
      tt:if="{{ loading }}"
      size="50%"
      custom-class="van-switch__loading"
    />
  </view>
</view>
